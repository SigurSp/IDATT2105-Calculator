r\">\r\n      <input type=\"text\" class = \"result\" :value=\"result\" @input=\"handleInput\">\r\n      <div class=\"buttons\">\r\n        <button class=\"number\" id=\"button-7\"\r\n                @click=\"handleClick('7')\">7</button>\r\n        <button class=\"number\" id=\"8\"\r\n                @click=\"handleClick('8')\">8</button>\r\n        <button class=\"number\" id=\"9\"\r\n                @click=\"handleClick('9')\">9</button>\r\n        <button class=\"operator\" id=\"divide\"\r\n                @click=\"handleOperatorClick('/')\">/</button>\r\n\r\n        <button class=\"number\" id=\"4\"\r\n                @click=\"handleClick('4')\">4</button>\r\n        <button class=\"number\" id=\"5\"\r\n                @click=\"handleClick('5')\">5</button>\r\n        <button class=\"number\" id=\"6\"\r\n                @click=\"handleClick('6')\">6</button>\r\n        <button class=\"operator\" id=\"multiply\"\r\n                @click=\"handleOperatorClick('*')\">*</button>\r\n\r\n        <button class=\"number\" id=\"1\"\r\n                @click=\"handleClick('1')\">1</button>\r\n        <button class=\"number\" id=\"2\"\r\n                @click=\"handleClick('2')\">2</button>\r\n        <button class=\"number\" id=\"3\"\r\n                @click=\"handleClick('3')\">3</button>\r\n        <button class=\"operator\" id=\"subtract\"\r\n                @click=\"handleOperatorClick('-')\">-</button>\r\n\r\n        <button class=\"number\" id=\"0\"\r\n                @click=\"handleClick('0')\">0</button>\r\n        <button class=\"number\" id=\".\"\r\n                @click=\"handleClick('.')\">.</button>\r\n        <button class=\"number\" id=\"00\"\r\n                @click=\"handleClick('00')\">00</button>\r\n\r\n        <button class=\"operator\" id=\"add\"\r\n                @click=\"handleOperatorClick('+')\">+</button>\r\n\r\n        <button class=\"clear\" id=\"clear\"\r\n                @click=\"handleClear\">C</button>\r\n        <button class=\"clear\" id=\"clearAll\"\r\n                @click=\"handleClearEntry\">CE</button>\r\n        <button class=\"equal\" id=\"equal\"\r\n                @click=\"calculate()\">=</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div id = \"calculatorHistory\">\r\n    <div class=\"calculatorHistory\">\r\n      <h2>History</h2>\r\n      <ul>\r\n        <li v-for=\"history in histories\" :key=\"history\">{{ history }}</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.calculator {\r\n  width: 300px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n}\r\n.result {\r\n  width: 90%;\r\n  padding: 10px;\r\n  font-size: 20px;\r\n  text-align: right;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.buttons {\r\n  display: grid;\r\n  grid-template-columns: repeat(4, 1fr);\r\n  grid-gap: 10px;\r\n}\r\nbutton {\r\n  padding: 15px;\r\n  font-size: 18px;\r\n  cursor: pointer;\r\n  border: none;\r\n  outline: none;\r\n}\r\n\r\n.number, operator {\r\n  background-color: #f0f0f0;\r\n}\r\n.clear, .equal {\r\n  background-color: #c42334;\r\n  color: #fff;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #ffffff;\r\n}\r\n\r\n.equal {\r\n  grid-column: span 2;\r\n}\r\n\r\n.calculatorHistory {\r\n  width: 300px;\r\n  height: 300px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  overflow: auto;\r\n}\r\n</style>\r\n"],"mappings":";AACA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,CAAC;MACfC,MAAM,EAAE,CAAC;MACTC,SAAS,EAAE,EAAC,CAAE;IAChB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACC,KAAK,EAAE;MACjB,IAAI,IAAI,CAACC,UAAU,EAAE;QACnB,IAAI,CAACL,MAAK,GAAII,KAAK;QACnB,IAAI,CAACC,UAAS,GAAI,KAAK;MACzB,OAAO;QACL,IAAI,CAACL,MAAK,IAAKI,KAAK;MACtB;IACF,CAAC;IACDE,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACN,MAAK,GAAI,EAAE;MAChB,IAAI