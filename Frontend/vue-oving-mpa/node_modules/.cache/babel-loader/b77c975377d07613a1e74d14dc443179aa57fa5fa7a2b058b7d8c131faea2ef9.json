{"ast":null,"code":"export default {\n  props: ['contact'],\n  computed: {\n    isFormInvalid() {\n      return !this.Name || !this.Email || !this.Message;\n    }\n  },\n  data() {\n    return {\n      Name: '',\n      Email: '',\n      Message: ''\n    };\n  },\n  methods: {\n    async formSubmitted() {\n      if (!this.Name || !this.Email || !this.Message) {\n        alert('Please fill in all fields');\n        return;\n      }\n      const formData = {\n        Name: this.Name,\n        Email: this.Email,\n        Message: this.Message\n      };\n      try {\n        const response = await fetch('/feedback', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(formData)\n        });\n        if (response.ok) {\n          const data = await response.json();\n          console.log(data);\n          alert('Form submitted');\n        } else {\n          alert('Failed to submit form');\n        }\n      } catch (error) {\n        console.error('Error submitting form:', error);\n        alert('Error submitting form');\n      }\n      localStorage.setItem('Name', this.Name);\n      localStorage.setItem('Email', this.Email);\n      alert('Form submitted');\n    }\n  },\n  created() {\n    console.log('Contact component created');\n    this.Name = localStorage.getItem('Name') || '';\n    this.Email = localStorage.getItem('Email') || '';\n  }\n};","map":{"version":3,"names":["props","computed","isFormInvalid","Name","Email","Message","data","methods","formSubmitted","alert","formData","response","fetch","method","headers","body","JSON","stringify","ok","json","console","log","error","localStorage","setItem","created","getItem"],"sources":["C:\\Users\\sigau\\OneDrive - NTNU\\Dataingeniør S4\\IDATT2105 Full-stack applikasjonsutvikling\\Øvinger\\Øving 3\\vue-oving-mpa\\src\\components\\Contact.vue"],"sourcesContent":["<script>\r\nexport default {\r\n  props: ['contact'],\r\n  computed: {\r\n    isFormInvalid() {\r\n      return !this.Name || !this.Email || !this.Message\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      Name : '',\r\n      Email : '',\r\n      Message : ''\r\n    }\r\n  },\r\n  methods: {\r\n    async formSubmitted() {\r\n      if (!this.Name || !this.Email || !this.Message) {\r\n        alert('Please fill in all fields');\r\n        return;\r\n      }\r\n\r\n      const formData = {\r\n        Name: this.Name,\r\n        Email: this.Email,\r\n        Message: this.Message\r\n      };\r\n\r\n      try {\r\n        const response = await fetch('/feedback', {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(formData),\r\n        });\r\n        if (response.ok) {\r\n          const data = await response.json();\r\n          console.log(data);\r\n          alert('Form submitted');\r\n        } else {\r\n          alert('Failed to submit form');\r\n        }\r\n      } catch (error) {\r\n        console.error('Error submitting form:', error);\r\n        alert('Error submitting form');\r\n      }\r\n\r\n      localStorage.setItem('Name', this.Name);\r\n      localStorage.setItem('Email', this.Email);\r\n      alert('Form submitted');\r\n    }\r\n  },\r\n  created() {\r\n    console.log('Contact component created');\r\n    this.Name = localStorage.getItem('Name') || '';\r\n    this.Email = localStorage.getItem('Email') || '';\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n<div id = \"contact\">\r\n  <div class=\"container mb-3\">\r\n    <form action=\"\">\r\n      <div class=\"mb-3\">\r\n        <label for=\"name\" class=\"form-label\">Name</label>\r\n        <input type=\"text\" class=\"form-control\" id=\"name\"\r\n        v-model = \"Name\">\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label for=\"email\" class=\"form-label\">Email</label>\r\n        <input type=\"email\" class=\"form-control\" id=\"email\" v-model=\"Email\">\r\n\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label for=\"message\" class=\"form-label\">Message</label>\r\n        <textarea class=\"form-control\" id=\"message\" rows=\"3\" v-model=\"Message\"></textarea>\r\n      </div>\r\n      <button\r\n          type=\"submit\"\r\n          class=\"btn btn-primary\"\r\n          :disabled=\"isFormInvalid\"\r\n          @click.prevent=\"formSubmitted\"\r\n      >SubmitForm</button>\r\n    </form>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AACA,eAAe;EACbA,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,CAAC,IAAI,CAACC,IAAG,IAAK,CAAC,IAAI,CAACC,KAAI,IAAK,CAAC,IAAI,CAACC,OAAM;IAClD;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLH,IAAG,EAAI,EAAE;MACTC,KAAI,EAAI,EAAE;MACVC,OAAM,EAAI;IACZ;EACF,CAAC;EACDE,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI,CAAC,IAAI,CAACL,IAAG,IAAK,CAAC,IAAI,CAACC,KAAI,IAAK,CAAC,IAAI,CAACC,OAAO,EAAE;QAC9CI,KAAK,CAAC,2BAA2B,CAAC;QAClC;MACF;MAEA,MAAMC,QAAO,GAAI;QACfP,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,OAAO,EAAE,IAAI,CAACA;MAChB,CAAC;MAED,IAAI;QACF,MAAMM,QAAO,GAAI,MAAMC,KAAK,CAAC,WAAW,EAAE;UACxCC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YAAE,cAAc,EAAE;UAAmB,CAAC;UAC/CC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACP,QAAQ;QAC/B,CAAC,CAAC;QACF,IAAIC,QAAQ,CAACO,EAAE,EAAE;UACf,MAAMZ,IAAG,GAAI,MAAMK,QAAQ,CAACQ,IAAI,CAAC,CAAC;UAClCC,OAAO,CAACC,GAAG,CAACf,IAAI,CAAC;UACjBG,KAAK,CAAC,gBAAgB,CAAC;QACzB,OAAO;UACLA,KAAK,CAAC,uBAAuB,CAAC;QAChC;MACF,EAAE,OAAOa,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;QAC9Cb,KAAK,CAAC,uBAAuB,CAAC;MAChC;MAEAc,YAAY,CAACC,OAAO,CAAC,MAAM,EAAE,IAAI,CAACrB,IAAI,CAAC;MACvCoB,YAAY,CAACC,OAAO,CAAC,OAAO,EAAE,IAAI,CAACpB,KAAK,CAAC;MACzCK,KAAK,CAAC,gBAAgB,CAAC;IACzB;EACF,CAAC;EACDgB,OAAOA,CAAA,EAAG;IACRL,OAAO,CAACC,GAAG,CAAC,2BAA2B,CAAC;IACxC,IAAI,CAAClB,IAAG,GAAIoB,YAAY,CAACG,OAAO,CAAC,MAAM,KAAK,EAAE;IAC9C,IAAI,CAACtB,KAAI,GAAImB,YAAY,CAACG,OAAO,CAAC,OAAO,KAAK,EAAE;EAClD;AACF","ignoreList":[]},"metadat