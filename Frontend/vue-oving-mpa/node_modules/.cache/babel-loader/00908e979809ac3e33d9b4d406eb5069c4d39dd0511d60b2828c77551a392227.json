ight') {\r\n        theme = 'dark';\r\n      }\r\n      this.theme = theme;\r\n      this.storeThemeSettings();\r\n    },\r\n    storeThemeSettings() {\r\n      localStorage.setItem('theme', this.theme);\r\n    },\r\n    getThemeSetting() {\r\n      let theme = localStorage.getItem('theme');\r\n      if (theme) {\r\n        this.theme = theme;\r\n      }\r\n    },\r\n    logOut(){\r\n      localStorage.removeItem('user');\r\n      authState.isLoggedIn = false;\r\n      this.$router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <nav :class=\"[`navbar-${theme}`, `bg-${theme}`, 'navbar', 'navbar-expand-lg']\">\r\n    <div class=\"container-fluid\">\r\n      <a class=\"navbar-brand\" href=\"#\">My Vue </a>\r\n      <ul class=\"navbar-nav me-auto mb-2 mb-lg-0\">\r\n\r\n        <navbar-link\r\n            v-for=\"(page, index) in pages\"\r\n            class=\"nav-item\"\r\n            :key=\"index\"\r\n            :page=\"page\"\r\n            :index=\"index\"\r\n            :isActive=\"index === activePage\"\r\n        ></navbar-link>\r\n\r\n      </ul>\r\n      <form class=\"d-flex\">\r\n        <button\r\n          class=\"btn btn-primary\"\r\n          @click.prevent=\"changeTheme()\"\r\n          >Toggle</button>\r\n        <button\r\n          v-if=\"isLoggedIn\"\r\n          class=\"btn btn-danger ms-2\"\r\n          @click.prevent=\"logOut\"\r\n          >Log out </button>\r\n      </form>\r\n    </div>\r\n  </nav>\r\n</template>\r\n"],"mappings":";AACA,OAAOA,UAAS,MAAO,kBAAkB;AACzC,SAASC,SAAQ,QAAS,YAAY;AACtC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,KAAK,EAAE,CAAC,OAAO,EAAE,YAAY,CAAC;EAC9BC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAOR,SAAS,CAACQ,UAAU;IAC7B;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAIJ,KAAI,GAAI,OAAO;MACnB,IAAI,IAAI,CAACA,KAAI,KAAM,OAAO,EAAE;QAC1BA,KAAI,GAAI,MAAM;MAChB;MACA,IAAI,CAACA,